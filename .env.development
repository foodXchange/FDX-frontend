# FDX Development Environment Configuration

# Build optimizations for faster development
GENERATE_SOURCEMAP=false
DISABLE_ESLINT_PLUGIN=true
TSC_COMPILE_ON_ERROR=true
REACT_APP_FAST_REFRESH=true
SKIP_PREFLIGHT_CHECK=true

# Development server settings
FAST_REFRESH=true
BROWSER=none
REACT_APP_NODE_ENV=development

# Backend API URL
REACT_APP_API_URL=http://localhost:3001/api
REACT_APP_WS_URL=ws://localhost:3001

# Feature flags for development
REACT_APP_ENABLE_DEVTOOLS=true
REACT_APP_ENABLE_DEBUG=true
REACT_APP_MOCK_DATA=true

# Performance monitoring
REACT_APP_ENABLE_PERFORMANCE_MONITORING=false

# Cache settings
REACT_APP_CACHE_ENABLED=true
REACT_APP_CACHE_DURATION=300000
