# Expert Marketplace Environment Variables

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:3001/api
REACT_APP_WS_URL=ws://localhost:3001

# Authentication
REACT_APP_JWT_SECRET=your-jwt-secret-key-here
REACT_APP_AUTH_DOMAIN=your-auth-domain.com

# Stripe Payment Integration
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
REACT_APP_STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# File Upload (AWS S3)
REACT_APP_AWS_REGION=us-east-1
REACT_APP_AWS_S3_BUCKET=expert-marketplace-files
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# Push Notifications (VAPID)
REACT_APP_VAPID_PUBLIC_KEY=your_vapid_public_key
VAPID_PRIVATE_KEY=your_vapid_private_key
REACT_APP_VAPID_SUBJECT=mailto:your-email@example.com

# Third-party Integrations
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
REACT_APP_SENTRY_DSN=your_sentry_dsn_for_error_tracking

# Feature Flags
REACT_APP_ENABLE_VIDEO_CALLS=true
REACT_APP_ENABLE_PAYMENTS=true
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=true
REACT_APP_ENABLE_FILE_UPLOAD=true

# Environment
NODE_ENV=development
REACT_APP_ENVIRONMENT=development

# Logging
REACT_APP_LOG_LEVEL=debug
REACT_APP_ENABLE_ANALYTICS=false

# Performance
REACT_APP_ENABLE_SW=true
REACT_APP_CACHE_STRATEGY=cacheFirst

# Security
REACT_APP_CSP_NONCE=your_csp_nonce
REACT_APP_ALLOWED_DOMAINS=localhost,yourdomain.com