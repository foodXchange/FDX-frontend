# FoodXchange Environment Configuration
REACT_APP_WS_URL=ws://localhost:3001
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_ENVIRONMENT=development
GENERATE_SOURCEMAP=false

# Azure AI Configuration
REACT_APP_AZURE_AI_ENDPOINT=https://your-endpoint.cognitiveservices.azure.com/
REACT_APP_AZURE_AI_KEY=your-api-key-here

# Feature Flags
REACT_APP_ENABLE_AI_FEATURES=true
REACT_APP_ENABLE_COMPLIANCE=true
REACT_APP_ENABLE_ANALYTICS=true

# Authentication
REACT_APP_AUTH_DOMAIN=foodxchange.com
REACT_APP_SESSION_TIMEOUT=3600000

# File Upload
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=csv,xlsx,xls,pdf,jpg,jpeg,png

# Debug
REACT_APP_DEBUG_MODE=false