    "zustand": "^5.0.6"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "type-check": "tsc --noEmit",
    "test:coverage": "craco test --coverage --watchAll=false",
    "test:ci": "CI=true craco test --coverage --watchAll=false",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "docker:build": "docker build -t fdx-frontend .",
    "docker:run": "docker run -p 8080:8080 fdx-frontend",
    "check:no-css": "! find src -name '*.css' -o -name '*.scss' -o -name '*.sass' | grep .",
    "check:no-classname": "! grep -r 'className=' src/ --include='*.tsx' --include='*.jsx' | grep .",
    "check:mui-only": "npm run check:no-css && npm run check:no-classname",
    "clean:deps": "rm -rf node_modules package-lock.json && npm install",
    "audit:fix": "npm audit fix --force",
    "update:deps": "npm update",
    "prebuild": "npm run type-check && npm run lint",
    "postbuild": "npm run analyze"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@eslint/js": "^9.17.0",
    "@types/dompurify": "^3.0.5",
    "@types/eslint__js": "^8.42.3",
    "@types/lodash": "^4.17.20",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "craco-alias": "^3.0.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-webpack-plugin": "^5.0.2",
    "msw": "^1.3.5",
    "prettier": "^3.4.2",
    "source-map-explorer": "^2.5.3",
    "typescript-eslint": "^8.18.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "resolutions": {
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7"
  }
}
