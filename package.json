    "@hookform/resolvers": "^5.1.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-data-grid": "^8.8.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.59",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.5",
    "i18next": "^23.16.8",
    "i18next-browser-languagedetector": "^7.2.2",
    "i18next-http-backend": "^2.7.3",
    "immer": "^10.1.1",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.60.0",
    "react-i18next": "^13.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.6.3",
    "react-scripts": "5.0.1",
    "tailwind-merge": "^3.3.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "xlsx": "^0.18.5",
    "zod": "^4.0.5",
    "zustand": "^5.0.6"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\"",
    "type-check": "tsc --noEmit",
    "test:coverage": "craco test --coverage --watchAll=false",
    "test:ci": "CI=true craco test --coverage --watchAll=false",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "docker:build": "docker build -t fdx-frontend .",
    "docker:run": "docker run -p 8080:8080 fdx-frontend"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@types/chart.js": "^2.9.41",
    "craco-alias": "^3.0.1"
  }
}
