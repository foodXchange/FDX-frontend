  "scripts": {
    "start": "react-scripts start",
    "start:craco": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --fix --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "type-check": "tsc --noEmit",
    "test:coverage": "jest --config jest.config.json --coverage",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "analyze": "cross-env ANALYZE=true npm run build",
    "analyze:bundle": "npm run build && npx bundle-analyzer build/static/js/*.js",
    "analyze:webpack": "webpack-bundle-analyzer build/static/js/*.js",
    "docker:build": "docker build -t fdx-frontend .",
    "docker:run": "docker run -p 8080:8080 fdx-frontend",
    "check:no-css": "! find src -name '*.css' -o -name '*.scss' -o -name '*.sass' | grep .",
    "check:no-classname": "! grep -r 'className=' src/ --include='*.tsx' --include='*.jsx' | grep .",
    "check:mui-only": "npm run check:no-css && npm run check:no-classname",
    "clean:deps": "rm -rf node_modules package-lock.json && npm install",
    "audit:fix": "npm audit fix --force",
    "update:deps": "npm update",
    "prebuild": "npm run type-check && npm run lint",
    "postbuild": "npm run analyze",
    "server": "ts-node server.ts",
    "server:prod": "node dist/server.js",
    "api": "ts-node api-server.ts",
    "api:prod": "node dist/api-server.js",
    "ws": "ts-node websocket-server.ts",
    "ws:prod": "node dist/websocket-server.js",
    "dev:all": "concurrently \"npm run start\" \"npm run api\" \"npm run ws\"",
    "build:server": "tsc --project tsconfig.node.json",
    "build:deploy": "ts-node build-for-deploy.ts",
    "cleanup:old-files": "ts-node scripts/cleanup-old-files.ts",
    "cleanup:old-files:dry": "ts-node scripts/cleanup-old-files.ts --dry-run",
    "cleanup:frontend": "ts-node scripts/clean-frontend.ts",
    "cleanup:icons": "ts-node scripts/fix-missing-icons.ts",
    "cleanup:all": "npm run cleanup:frontend && npm run cleanup:icons && npm run lint:fix",
    "generate-favicon": "ts-node scripts/generate-favicon.ts",
    "build:analyze": "ANALYZE=true npm run build",
    "build:prod": "NODE_ENV=production npm run build",
    "optimize": "npm run lint:fix && npm run type-check && npm run build:prod",
    "size-check": "npx bundlesize",
    "test:unit": "jest --config jest.config.json",
    "test:watch": "jest --config jest.config.json --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:unit && npm run test:e2e",
    "monitor:dev": "cross-env REACT_APP_ENABLE_MONITORING=true npm start",
    "monitor:build": "cross-env REACT_APP_ENABLE_MONITORING=true npm run build",
    "analyze:source-map": "npm run build && npx source-map-explorer build/static/js/*.js",
    "build:optimized": "cross-env GENERATE_SOURCEMAP=false npm run build",
    "perf:audit": "lighthouse http://localhost:3000 --output=html --output-path=./performance-audit.html",
    "bundle:size": "npm run build && bundlesize",
    "security:audit": "npm audit --audit-level moderate",
    "security:scan": "npm run build && bundlesize",
    "security:test": "npm run test -- --testPathPattern=security",
    "security:report": "npm audit --json > security-audit.json",
    "start:mock": "REACT_APP_USE_MOCK_API=true npm start",
    "start:dev": "REACT_APP_API_URL=http://localhost:5000/api npm start",
    "start:staging": "REACT_APP_API_URL=https://staging-api.yourapp.com/api npm start",
    "build:mock": "REACT_APP_USE_MOCK_API=true npm run build",
    "test:api": "node scripts/test-api-integration.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.54.1",
